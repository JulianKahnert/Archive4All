[Defaults]

; Copy or move files to the archive
copy_or_move            = copy

; When processing directores, recurse into subdirs
recursive_directories   = True

; Add all used tags from from the archive
gather_archive_tags     = False

; Sort the tags from the archive alphabetically
sort_gathered_tags      = False

; Overwrite the tag list from this config file
overwrite_config_tags   = False


[Directories]

; Multiple input paths can be defined by being placed in a JSON list
input_paths = ["~/Documents/test_documents" ]

; Only one output path is allowed (therefore no JSON here)
output_path = ~/Downloads/test_archive/

[Tags]
tag1
tag2
tag3
